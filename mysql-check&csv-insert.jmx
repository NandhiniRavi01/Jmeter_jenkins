          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor">
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">// Define expected records
String[][] expectedRecords = {
    {&quot;1&quot;, &quot;xxxxxx&quot;, &quot;xxxxxx@example.com&quot;},
    {&quot;2&quot;, &quot;xxxxxx&quot;, &quot;xxxxxx@example.com&quot;},
    {&quot;3&quot;, &quot;xxxxxx&quot;, &quot;xxxxxx@example.com&quot;},
    {&quot;4&quot;, &quot;xxxxxx&quot;, &quot;xxxxxx@example.com&quot;}
    
};

// Track how many matches found
int totalRows = expectedRecords.length;
boolean[] found = new boolean[totalRows];

// Loop through rows returned from DB
int row = 1;
while (true) {
    String id = vars.get(&quot;userData_id_&quot; + row);
    if (id == null) break; // No more rows

    String username = vars.get(&quot;userData_username_&quot; + row);
    String email = vars.get(&quot;userData_email_&quot; + row);

    for (int i = 0; i &lt; totalRows; i++) {
        if (!found[i]) {
            if (expectedRecords[i][0].equals(id) &amp;&amp;
                expectedRecords[i][1].equals(username) &amp;&amp;
                expectedRecords[i][2].equals(email)) {
                found[i] = true;
                break;
            }
        }
    }
    row++;
}

// Check if any expected record was not found
for (int i = 0; i &lt; totalRows; i++) {
    if (!found[i]) {
        Failure = true;
        FailureMessage = &quot;Missing expected record: id=&quot; + expectedRecords[i][0]
            + &quot;, username=&quot; + expectedRecords[i][1]
            + &quot;, email=&quot; + expectedRecords[i][2];
        break;
    }
}
</stringProp>
